{
    "$schema": "https://schemas.botframework.com/schemas/component/v1.0/component.schema",
    "$role": "implements(Microsoft.IDialog)",
    "title": "Find Albion User",
    "description": "This will search the Albion Online API for a specific user",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "Username": {
            "$ref": "schema:#/definitions/stringExpression",
            "title": "Username",
            "description": "The username to look up"
        },
        "UserFound": {
            "$ref": "schema:#/definitions/stringExpression",
            "title": "User Found",
            "description": "A boolean as to whether or not the user was found"
        },
        "UserId": {
            "$ref": "schema:#/definitions/stringExpression",
            "title": "User Id",
            "description": "The Id of the user found"
        },
        "PvPFame": {
            "$ref": "schema:#/definitions/stringExpression",
            "title": "PvP Fame",
            "description": "The total kill fame of the found user"
        },
        "PvEFame": {
            "$ref": "schema:#/definitions/stringExpression",
            "title": "PvE Fame",
            "description": "The total mob fame of the found user"
        },
        "CraftingFame": {
            "$ref": "schema:#/definitions/stringExpression",
            "title": "Crafting Fame",
            "description": "The total amount of crafting fame of the found user"
        },
        "GatheringFame": {
            "$ref": "schema:#/definitions/stringExpression",
            "title": "Gathering Fame",
            "description": "The total amount of gathering fame of the found user"
        }
    }
}